<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
            prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.seventhseagenerator.Controllers.chooseNationController">
    <children>
        <Label layoutX="259.0" layoutY="50.0" prefHeight="55.0" prefWidth="248.0" text="Choose a Nation">
            <font>
                <Font name="Bauhaus 93" size="31.0"/>
            </font>
        </Label>
        <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="14.0" layoutY="30.0" pickOnBounds="true"
                   preserveRatio="true">
            <image>
                <Image url="@horseBadge.jpg"/>
            </image>
        </ImageView>
        <Button fx:id="continueButton" layoutX="521.0" layoutY="361.0" mnemonicParsing="false" onAction="#onClick"
                text="Continue"/>
        <Button fx:id="avalon" layoutX="86.0" layoutY="254.0" mnemonicParsing="false" onAction="#onAvalon"
                prefHeight="96.0" prefWidth="104.0" text="Avalon">
            <font>
                <Font name="Bauhaus 93" size="14.0"/>
            </font>
        </Button>
        <Label fx:id="nationDescription" layoutX="194.0" layoutY="105.0" prefHeight="118.0" prefWidth="295.0"
               text="Label" visible="false" wrapText="true"/>
        <Button fx:id="castille" layoutX="207.0" layoutY="254.0" mnemonicParsing="false" onAction="#onCastille"
                prefHeight="96.0" prefWidth="104.0" text="Castille">
            <font>
                <Font name="Bauhaus 93" size="14.0"/>
            </font>
        </Button>
        <Button fx:id="vodacce" layoutX="322.0" layoutY="254.0" mnemonicParsing="false" onAction="#onVodacce"
                prefHeight="96.0" prefWidth="104.0" text="Vodacce">
            <font>
                <Font name="Bauhaus 93" size="14.0"/>
            </font>
        </Button>
        <Button fx:id="montaigne" layoutX="437.0" layoutY="254.0" mnemonicParsing="false" onAction="#onMontaigne"
                prefHeight="96.0" prefWidth="104.0" text="Montaigne">
            <font>
                <Font name="Bauhaus 93" size="14.0"/>
            </font>
        </Button>
    </children>
</AnchorPane>
